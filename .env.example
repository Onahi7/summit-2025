APP_NAME="NAPPS Conference"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://napps.${DROPLET_IP}.nip.io

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Supabase Configuration
VITE_SUPABASE_URL=your-project-url
VITE_SUPABASE_ANON_KEY=your-anon-key

# Database Configuration (Supabase PostgreSQL)
DB_CONNECTION=pgsql
DB_HOST=db.your-project-ref.supabase.co
DB_PORT=5432
DB_DATABASE=postgres
DB_USERNAME=postgres
DB_PASSWORD=your-database-password

# Application Configuration
BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Session and Security
SESSION_SECURE_COOKIE=false
SESSION_DOMAIN=.${DROPLET_IP}.nip.io
SANCTUM_STATEFUL_DOMAINS=${DROPLET_IP}.nip.io

# Digital Ocean Configuration
DROPLET_IP=your-droplet-ip

# Frontend Configuration
VITE_APP_URL=http://napps.${DROPLET_IP}.nip.io
VITE_APP_NAME="NAPPS Conference"
